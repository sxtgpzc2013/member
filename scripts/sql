BEGIN
	DECLARE sTemp VARCHAR(1000);
	DECLARE sTempPar VARCHAR(1000); 
	SET sTemp = ''; 
	SET sTempPar =rootId; 
	
	WHILE sTempPar is not null DO 
		IF sTemp != '' THEN
			SET sTemp = concat(sTemp,',',sTempPar);
		ELSE
			SET sTemp = sTempPar;
		END IF;

		SET sTemp = concat(sTemp,',',sTempPar); 
		SELECT group_concat(tuijiannumber) INTO sTempPar FROM zx_member where tuijiannumber<>usernumber and FIND_IN_SET(usernumber,sTempPar)>0; 
	END WHILE; 
	
RETURN sTemp; 
END